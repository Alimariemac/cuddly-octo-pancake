{
  "name": "womens-website",
  "version": "1.0.0",
  "description": "a website to connect women with the support they need",
  "main": "index.js",
  "scripts": {
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/",
